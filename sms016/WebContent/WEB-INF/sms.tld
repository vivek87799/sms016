<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
 <tlib-version>1.0</tlib-version>
 <jsp-version>2.0</jsp-version>
 <short-name>p</short-name>
 <display-name>Label</display-name>
 
 <tag>
  <name>Label</name>
  <tag-class>com.sms.customtags.LabelTag</tag-class>
  <body-content>empty</body-content>
  <attribute>
   <name>label</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
 </tag>
 
 <tag>
 <name>TeacherTable</name>
 <tag-class>com.sms.customtags.RetreiveTeacherDetails</tag-class>
 <body-content>empty</body-content>
 </tag>
 
 <tag>
  <name>Button</name>
  <tag-class>com.sms.customtags.ButtonTags</tag-class>
  <body-content>empty</body-content>
  <attribute>
   <name>name</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>value</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>type</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>onclick</name>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
 </tag>
 
 <tag>
  <name>Link</name>
  <tag-class>com.sms.customtags.LinkTag</tag-class>
  <body-content>empty</body-content>
  <attribute>
   <name>href</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
  <attribute>
   <name>linkText</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
 </tag>
 
 <tag>
  <name>Text</name>
  <tag-class>com.sms.customtags.TextTag</tag-class>
  <body-content>empty</body-content>
  <attribute>
   <name>text</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
  </attribute>
 </tag>
 
</taglib>
